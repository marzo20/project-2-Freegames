<div >
    <h2 class="
    grid grid-cols-2 m-5 gap-5
    ">
    <% results.forEach(result => {%>
        <img src="<%= result.thumbnail%>" alt="game poster">
       
        <ul>
            <li>
                Title: <%= result.title%>
            </li>
            <li>
                Platform: <%= result.platform%>
            </li>
            <li>
                Genre: <%= result.genre%>
            </li>
            <li>
                Description: <%= result.short_description%>
            </li>
            <li>
                <a href="<%= result.game_url%>">GO PLAY!</a>
            </li>
        </ul>
        <%})%>
    </h2>
</div>